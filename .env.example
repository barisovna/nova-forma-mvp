# Runtime
PORT=3000
NODE_ENV=development

# LLM provider switch: deepseek | qwen
LLM_PROVIDER=deepseek

# Secrets (set real values in Vercel env / local .env.local)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
QWEN_API_KEY=your_qwen_api_key_here
JWT_SECRET=your_jwt_secret_here
DATABASE_URL=your_database_url_here
REDIS_URL=your_redis_url_here
WEB_PUSH_PUBLIC_KEY=your_web_push_public_key_here
WEB_PUSH_PRIVATE_KEY=your_web_push_private_key_here
PAYMENT_SECRET_KEY=your_payment_secret_key_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
